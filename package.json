{
  "name": "tobeit67",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "home:dev": "",
    "presetup": "cp .env.example .env.template",
    "setup": "pnpm i && pnpm run setup:env && docker compose up -d && pnpm run db:migrate && pnpm run db:generate",
    "setup:env": "cp .env.template .env && pnpm run update:env",
    "update:env": "cp .env packages/airtable-api/.env && cp .env apps/regis/.env && cp .env packages/firebase-auth-api/.env",
    "db:migrate": "npx prisma migrate dev --schema apps/regis/prisma/schema.prisma",
    "db:generate": "npx prisma generate --schema apps/regis/prisma/schema.prisma"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "prisma": "4.4.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.13.2"
}
