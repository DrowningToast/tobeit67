{
  "kind": "collectionType",
  "collectionName": "class_slots",
  "info": {
    "singularName": "class-slot",
    "pluralName": "class-slots",
    "displayName": "ClassSlot",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "callsign": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "start": {
      "type": "datetime",
      "required": true
    },
    "end": {
      "type": "datetime",
      "required": true
    },
    "slotId": {
      "type": "uid",
      "targetField": "callsign",
      "required": true
    },
    "class": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::class.class",
      "inversedBy": "class_slots"
    },
    "classNumber": {
      "type": "string",
      "required": true
    },
    "reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "class_slot"
    }
  }
}
